{
  "name": "vue-aloof-forms",
  "private": false,
  "version": "0.1.0",
  "author": "Royal Bissell",
  "description": "## Project Setup ``` npm install ```",
  "type": "module",
  "files": [
    "dist",
    "types/*",
    "*.json"
  ],
  "main": "./dist/vue-aloof-forms.umd.js",
  "module": "./dist/vue-aloof-forms.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue-aloof-forms.es.js",
      "require": "./dist/vue-aloof-forms.umd.js"
    },
    "./dist/vue-aloof-forms.css": {
      "import": "./dist/vue-aloof-forms.css",
      "require": "./dist/vue-aloof-forms.css"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "build:lib && build:styles",
    "build:lib": "vite build && vue-tsc",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/node": "^18.15.3",
    "@vitejs/plugin-vue": "^4.1.0",
    "path": "^0.12.7",
    "rollup-plugin-typescript2": "^0.34.1",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-dts": "^2.1.0",
    "vue-tsc": "^1.2.0"
  },
  "peerDependencies": {
    "tailwindcss": "^3.2.7",
    "vue": "^3.2.47"
  }
}
